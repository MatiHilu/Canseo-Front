C,YAAL,GAAoB,2EAApB;AACD,OATH;AAUD;AAxBM;AATI,CAAf","sourcesContent":["<template>\r\n  <div class=\"password-form\">\r\n    <h2>Cambiar Contraseña</h2>\r\n    <form @submit.prevent=\"submitForm\">\r\n      <div class=\"form-group\">\r\n        <label for=\"current-password\">Contraseña Actual:</label>\r\n        <input type=\"password\" id=\"current-password\" v-model=\"currentPassword\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"new-password\">Nueva Contraseña:</label>\r\n        <input type=\"password\" id=\"new-password\" v-model=\"newPassword\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"confirm-password\">Confirmar Contraseña:</label>\r\n        <input type=\"password\" id=\"confirm-password\" v-model=\"confirmPassword\" required>\r\n      </div>\r\n      <button type=\"submit\">Guardar Cambios</button>\r\n      <p v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ClienteService from \"@/services/ClientesService\";\r\nimport store from \"@/store\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPassword: \"\",\r\n      newPassword: \"\",\r\n      confirmPassword: \"\",\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      if (this.newPassword !== this.confirmPassword) {\r\n        this.errorMessage = \"Las contraseñas no coinciden.\";\r\n        return;\r\n      }\r\n\r\n      const userId = store.getters.getUserId;\r\n      const cambioContraseñaData = {\r\n        currentPassword: this.currentPassword,\r\n        newPassword: this.newPassword,\r\n        confirmPassword: this.confirmPassword,\r\n      };\r\n\r\n      ClienteService.changePassword(userId, cambioContraseñaData)\r\n        .then(() => {\r\n          this.errorMessage = \"\";\r\n          alert(\"Contraseña cambiada exitosamente.\");\r\n          // Realizar acciones adicionales después de cambiar la contraseña (redireccionar, mostrar mensaje, etc.)\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al cambiar la contraseña:\", error);\r\n          this.errorMessage = \"Error al cambiar la contraseña. Por favor, verifica los datos ingresados.\";\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.password-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput[type=\"password\"] {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #000;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton[type=\"submit\"] {\r\n  border-radius: 5px;\r\n  border: 1px solid #8DBD47;\r\n  background-color: #8DBD47;\r\n  font-size: 14px;\r\n  padding: 3px 15px;\r\n  width: 100%;\r\n  height: 40px;\r\n  margin-top: 15px;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n  color: #000;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  margin-top: 5px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}