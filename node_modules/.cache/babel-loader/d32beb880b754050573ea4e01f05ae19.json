MAAA,YAAY,CAAC,MAAb,GACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,QAAA,MAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAtB;AACD,OAHH,EAIG,KAJH,CAIS,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OANH;AAOD,KAlBM;AAmBP,IAAA,cAnBO,4BAmBU;AACf,UAAM,YAAW,GAAI;AACnB,QAAA,MAAM,EAAE,KAAK,MADM;AAEnB,QAAA,QAAQ,EAAE,KAAK,QAFI;AAGnB,QAAA,KAAK,EAAE,KAAK,KAHO;AAInB,QAAA,QAAQ,EAAE,KAAK,QAJI;AAKnB,QAAA,SAAS,EAAE,KAAK,SALG;AAMnB,QAAA,QAAQ,EAAE,KAAK,QANI;AAOnB,QAAA,SAAS,EAAE,KAAK,QAPG;AAQnB,QAAA,YAAY,EAAE,KAAK,WARA;AASnB,QAAA,UAAU,EAAE,KAAK,SATE;AAUnB,QAAA,OAAO,EAAE,KAAK;AAVK,OAArB;AAaA,MAAA,eAAe,CAAC,MAAhB,CAAuB,YAAvB,EACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,iCAAZ,EAA+C,QAAQ,CAAC,IAAxD,EADgB,CAEhB;AACD,OAJH,EAKG,KALH,CAKS,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,+BAAZ,EAA6C,KAA7C,EADc,CAEd;AACD,OARH;AASF;AA1CO;AAtBI,CAAf","sourcesContent":["<template>\r\n    <div class=\"add-cliente\">\r\n      <h1>Agregar Cliente</h1>\r\n      <form @submit.prevent=\"agregarCliente\">\r\n        <div class=\"form-group\">\r\n          <label for=\"nombre\">Nombre:</label>\r\n          <input type=\"text\" id=\"nombre\" v-model=\"nombre\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"apellido\">Apellido:</label>\r\n          <input type=\"text\" id=\"apellido\" v-model=\"apellido\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">Email:</label>\r\n          <input type=\"email\" id=\"email\" v-model=\"email\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"telefono\">Teléfono:</label>\r\n          <input type=\"text\" id=\"telefono\" v-model=\"telefono\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"direccion\">Dirección:</label>\r\n          <input type=\"text\" id=\"direccion\" v-model=\"direccion\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">Contraseña:</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"id_barrio\">Barrio:</label>\r\n          <select id=\"id_barrio\" v-model=\"idBarrio\" required>\r\n            <option value=\"\">Seleccione un barrio</option>\r\n            <option v-for=\"barrio in barrios\" :key=\"barrio.id\" :value=\"barrio.id\">{{ barrio.nombre }}</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"nombre_perro\">Nombre del Perro:</label>\r\n          <input type=\"text\" id=\"nombre_perro\" v-model=\"nombrePerro\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"edad_perro\">Edad del Perro:</label>\r\n          <input type=\"number\" id=\"edad_perro\" v-model=\"edadPerro\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"id_raza\">Raza:</label>\r\n          <select id=\"id_raza\" v-model=\"idRaza\" required>\r\n            <option value=\"\">Seleccione una raza</option>\r\n            <option v-for=\"raza in razas\" :key=\"raza.id\" :value=\"raza.id\">{{ raza.nombre }}</option>\r\n          </select>\r\n        </div>\r\n        <button type=\"submit\">Agregar</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import ClientesService from \"@/services/ClientesService\";\r\n  import BarriosService from \"@/services/BarriosService\";\r\n  import RazasService from \"@/services/RazasService\";\r\n  \r\n  export default {\r\n    name: \"AddCliente\",\r\n    data() {\r\n      return {\r\n        nombre: \"\",\r\n        apellido: \"\",\r\n        email: \"\",\r\n        telefono: \"\",\r\n        direccion: \"\",\r\n        password: \"\",\r\n        idBarrio: \"\",\r\n        nombrePerro: \"\",\r\n        edadPerro: null,\r\n        idRaza: \"\",\r\n        barrios: [],\r\n        razas: []\r\n      };\r\n    },\r\n    mounted() {\r\n      this.fetchBarrios();\r\n      this.fetchRazas();\r\n    },\r\n    methods: {\r\n      fetchBarrios() {\r\n        BarriosService.getAll()\r\n          .then(response => {\r\n            this.barrios = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.log(error);\r\n          });\r\n      },\r\n      fetchRazas() {\r\n        RazasService.getAll()\r\n          .then(response => {\r\n            this.razas = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.log(error);\r\n          });\r\n      },\r\n      agregarCliente() {\r\n        const nuevoCliente = {\r\n          nombre: this.nombre,\r\n          apellido: this.apellido,\r\n          email: this.email,\r\n          telefono: this.telefono,\r\n          direccion: this.direccion,\r\n          password: this.password,\r\n          id_barrio: this.idBarrio,\r\n          nombre_perro: this.nombrePerro,\r\n          edad_perro: this.edadPerro,\r\n          id_raza: this.idRaza\r\n        };\r\n  \r\n        ClientesService.create(nuevoCliente)\r\n          .then(response => {\r\n            console.log(\"Cliente agregado exitosamente: \", response.data);\r\n            // Realizar acciones adicionales después de agregar un cliente (redireccionar, mostrar mensaje, etc.)\r\n          })\r\n          .catch(error => {\r\n            console.log(\"Error al agregar el cliente: \", error);\r\n            // Manejar el error y mostrar un mensaje de error al usuario si es necesario\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .add-cliente {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  h1 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input, select {\r\n    width: 100%;\r\n    padding: 5px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  button {\r\n    padding: 8px 12px;\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"sourceRoot":""}]}