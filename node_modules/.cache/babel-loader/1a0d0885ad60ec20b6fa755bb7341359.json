AtB,EAAoC,CAAA;AAAA;AAApC,G;;;;uBAjBJ,mBAAA,CAmBM,KAnBN,EAAA,UAAA,EAmBM,CAlBJ,UAkBI,EAjBJ,mBAAA,CAgBO,MAhBP,EAgBO;AAhBA,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,KAAA,IAAA,QAAA,CAAA,KAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAe,CAAA,SAAA,CAAf,CAAA;AAgBN,GAhBP,E,CACE,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,mBAAA,CAAwD,OAAxD,EAAwD;AAAjD,IAAA,IAAI,EAAC,OAA4C;AAApC,IAAA,EAAE,EAAC,OAAiC;;aAAhB,KAAA,CAAA,K,GAAK,M;MAAW;AAAT,IAAA,QAAQ,EAAR;AAAS,GAAxD,E,IAAA,E;;AAAA,G,iBAAwC,KAAA,CAAA,K,GACpC,CAHN,C,EAIA,mBAAA,CASM,KATN,EAAA,UAAA,EASM,CARJ,UAQI,EAPJ,mBAAA,CAMM,KANN,EAAA,UAAA,EAMM,C,gBALJ,mBAAA,CAA2E,OAA3E,EAA2E;AAApE,IAAA,EAAE,EAAC,UAAiE;;aAA7C,KAAA,CAAA,Q,GAAQ,M;MAAqC;AAAlC,IAAA,IAAI,EAAE,QAAA,CAAA,iBAA4B;AAAT,IAAA,QAAQ,EAAR;AAAS,GAA3E,E,IAAA,E;;AAAA,I,UAAA,C,oBAA8B,KAAA,CAAA,Q,GAK1B,EAJJ,mBAAA,CAGS,QAHT,EAGS;AAHD,IAAA,IAAI,EAAC,QAGJ;AAHc,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,wBAAA,IAAA,QAAA,CAAA,wBAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGnB,GAHT,E,CACa,KAAA,CAAA,Y,kBAAX,mBAAA,CAAwG,KAAxG,EAAA,UAAA,C,mBACA,mBAAA,CAA2F,KAA3F,EAAA,WAAA,C,EAFF,CAII,CANN,CAOI,CATN,C,EAUA,W,CAfF,E;;AAAA,GAiBI,CAnBN,C","sourcesContent":["<template>\r\n  <div class=\"login-paseador\">\r\n    <h1>Iniciar sesión como Paseador</h1>\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input type=\"email\" id=\"email\" v-model=\"email\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">Contraseña:</label>\r\n        <div class=\"password-input\">\r\n          <input id=\"password\" v-model=\"password\" :type=\"passwordFieldType\" required>\r\n          <button type=\"button\" @click=\"togglePasswordVisibility\">\r\n            <img v-if=\"showPassword\" src=\"http://localhost:8081/uploads/show-password.png\" alt=\"Mostrar contraseña\">\r\n            <img v-else src=\"http://localhost:8081/uploads/hide-password.png\" alt=\"Ocultar contraseña\">\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\">Iniciar sesión</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport store from \"@/store/index\";\r\n\r\nexport default {\r\n  name: \"LoginPaseador\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      showPassword: false\r\n    };\r\n  },\r\n  computed: {\r\n    passwordFieldType() {\r\n      return this.showPassword ? \"text\" : \"password\";\r\n    }\r\n  },\r\n  methods: {\r\n    login() {\r\n      const credentials = {\r\n        email: this.email,\r\n        password: this.password\r\n      };\r\n\r\n      PaseadoresService.login(credentials)\r\n        .then(response => {\r\n          const clientId = response.data.clientId;\r\n          store.dispatch('updateClientId', clientId);\r\n          localStorage.setItem(\"Authorization\", 'Bearer ' + response.data.token);\r\n          // Redireccionar a la página de paseador autenticado\r\n          this.$router.push(\"/reservas-paseador\");\r\n        })\r\n        .catch(error => {\r\n          console.log(\"Error al iniciar sesión como paseador: \", error);\r\n          // Mostrar mensaje de error al usuario si es necesario\r\n        });\r\n    },\r\n    togglePasswordVisibility() {\r\n      this.showPassword = !this.showPassword;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-paseador {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.password-input {\r\n  position: relative;\r\n}\r\n\r\n.password-input button {\r\n  position: absolute;\r\n  top: 50%;\r\n  right: 5px;\r\n  transform: translateY(-50%);\r\n  background-color: transparent;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.password-input button:focus {\r\n  outline: none;\r\n}\r\n\r\n.password-input img {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}