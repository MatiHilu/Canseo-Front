A,SAAS,EAAE,KAAK,SALI;AAMpB,QAAA,QAAQ,EAAE,KAAK,QANK;AAOpB,QAAA,SAAS,EAAE,KAAK;AAPI,OAAtB;AAUA,MAAA,iBAAiB,CAAC,MAAlB,CAAyB,aAAzB,EACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,kCAAZ,EAAgD,QAAQ,CAAC,IAAzD,EADgB,CAEhB;AACD,OAJH,EAKG,KALH,CAKS,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,gCAAZ,EAA8C,KAA9C,EADc,CAEd;AACD,OARH;AASF;AA9BO;AAjBI,CAAf","sourcesContent":["<template>\r\n  <div class=\"add-paseador\">\r\n    <h1>Agregar Paseador</h1>\r\n    <form @submit.prevent=\"agregarPaseador\">\r\n      <div class=\"form-group\">\r\n        <label for=\"nombre\">Nombre:</label>\r\n        <input type=\"text\" id=\"nombre\" v-model=\"nombre\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"apellido\">Apellido:</label>\r\n        <input type=\"text\" id=\"apellido\" v-model=\"apellido\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input type=\"email\" id=\"email\" v-model=\"email\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"telefono\">Teléfono:</label>\r\n        <input type=\"text\" id=\"telefono\" v-model=\"telefono\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"direccion\">Dirección:</label>\r\n        <input type=\"text\" id=\"direccion\" v-model=\"direccion\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">Contraseña:</label>\r\n        <input type=\"password\" id=\"password\" v-model=\"password\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"id_barrio\">Barrio:</label>\r\n        <select id=\"id_barrio\" v-model=\"idBarrio\" required>\r\n          <option value=\"\">Seleccione un barrio</option>\r\n          <option v-for=\"barrio in barrios\" :key=\"barrio.id\" :value=\"barrio.id\">{{ barrio.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <button style=\"background: black !important;\" type=\"submit\">Agregar</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport BarriosService from \"@/services/BarriosService\";\r\n\r\nexport default {\r\n  name: \"AddPaseador\",\r\n  data() {\r\n    return {\r\n      nombre: \"\",\r\n      apellido: \"\",\r\n      email: \"\",\r\n      telefono: \"\",\r\n      direccion: \"\",\r\n      password: \"\",\r\n      idBarrio: \"\",\r\n      barrios: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchBarrios();\r\n  },\r\n  methods: {\r\n    fetchBarrios() {\r\n      BarriosService.getAll()\r\n        .then(response => {\r\n          this.barrios = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    agregarPaseador() {\r\n      const nuevoPaseador = {\r\n        nombre: this.nombre,\r\n        apellido: this.apellido,\r\n        email: this.email,\r\n        telefono: this.telefono,\r\n        direccion: this.direccion,\r\n        password: this.password,\r\n        id_barrio: this.idBarrio\r\n      };\r\n\r\n      PaseadoresService.create(nuevoPaseador)\r\n        .then(response => {\r\n          console.log(\"Paseador agregado exitosamente: \", response.data);\r\n          // Realizar acciones adicionales después de agregar un paseador (redireccionar, mostrar mensaje, etc.)\r\n        })\r\n        .catch(error => {\r\n          console.log(\"Error al agregar el paseador: \", error);\r\n          // Manejar el error y mostrar un mensaje de error al usuario si es necesario\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.add-paseador {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput, select {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}