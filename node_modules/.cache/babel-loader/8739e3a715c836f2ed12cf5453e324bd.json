B,mBAAA,CAAkG,QAAlG,EAAkG;AAA/D,MAAA,KAAK,EAAE,MAAM,CAAC,EAAiD;AAA5C,MAAA,GAAG,EAAE,MAAM,CAAC;AAAgC,KAAlG,E,iBAAyE,MAAM,CAAC,M,CAAhF,EAAsF;AAAA;AAAtF,MAAsF,WAAtF,C;GAAkG,CAAlG,E;;AAAA,G,EADF,E;;AAAA,G,mBAA6B,KAAA,CAAA,mBAAA,CAAoB,S,GAG7C,CALN,C,EAMA,mBAAA,CAGM,KAHN,EAGM,IAHN,EAGM,CAFJ,WAEI,EADJ,mBAAA,CAA2D,QAA3D,EAA2D;AAAnD,IAAA,IAAI,EAAC,QAA8C;AAApC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA+B,GAA3D,EAA0C,UAA1C,CACI,CAHN,C,CA/BF,E;;AAAA,G,qCAwCI,EAJJ,mBAAA,CAEM,KAFN,EAEM,IAFN,EAEM,CADJ,mBAAA,CAA6E,QAA7E,EAA6E;AAArE,IAAA,IAAI,EAAC,QAAgE;AAAtD,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,kBAAA,IAAA,QAAA,CAAA,kBAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAiD,GAA7E,EAAkD,oBAAlD,CACI,CAFN,CAII,EADwB,KAAA,CAAA,gB,kBAA5B,YAAA,CAAiI,+BAAjI,EAAiI;UAAA;AAAlF,IAAA,gBAAe,EAAE,KAAA,CAAA,cAAiE;AAAhD,IAAA,sBAAsB,EAAE,KAAA,CAAA;AAAwB,GAAjI,E,IAAA,E;;AAAA,I,8CAAA,C,qCACI,CAvDN,C","sourcesContent":["<template>\r\n  <NavPaseador/>\r\n  <div class=\"perfil-paseador\">\r\n    <h1>Perfil del Paseador</h1>\r\n    <div>\r\n      <p>Nombre: {{ paseador.nombre }}</p>\r\n      <p>Apellido: {{ paseador.apellido }}</p>\r\n      <p>Email: {{ paseador.email }}</p>\r\n      <p>Teléfono: {{ paseador.telefono }}</p>\r\n      <p>Dirección: {{ paseador.direccion }}</p>\r\n      <p>Descripción: {{ paseador.descripcion }}</p>\r\n      <p>Mi Barrio: {{ paseador.nombre_barrio }}</p>\r\n      <p>Foto: {{ paseador.foto_perfil }}</p>\r\n      <img width=\"150\" :src=\"getImageUrl(paseador.foto_perfil)\" alt=\"Foto de perfil\">\r\n    \r\n    </div>\r\n    <button @click=\"showEditForm\">Editar perfil</button>\r\n    <form v-if=\"showForm\" @submit.prevent=\"updateProfile\">\r\n      <div class=\"form-group\">\r\n        <label for=\"nombre\">Nombre:</label>\r\n        <input type=\"text\" id=\"nombre\" v-model=\"updatedPaseadorData.nombre\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"apellido\">Apellido:</label>\r\n        <input type=\"text\" id=\"apellido\" v-model=\"updatedPaseadorData.apellido\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input type=\"email\" id=\"email\" v-model=\"updatedPaseadorData.email\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"telefono\">Teléfono:</label>\r\n        <input type=\"text\" id=\"telefono\" v-model=\"updatedPaseadorData.telefono\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"direccion\">Dirección:</label>\r\n        <input type=\"text\" id=\"direccion\" v-model=\"updatedPaseadorData.direccion\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"descripcion\">Descripción:</label>\r\n        <textarea id=\"descripcion\" v-model=\"updatedPaseadorData.descripcion\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"barrio\">Barrio:</label>\r\n        <select id=\"barrio\" v-model=\"updatedPaseadorData.id_barrio\" required>\r\n          <option v-for=\"barrio in barrios\" :value=\"barrio.id\" :key=\"barrio.id\">{{ barrio.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <div>\r\n        <button type=\"submit\">Guardar cambios</button>\r\n        <button type=\"button\" @click=\"cancelEdit\">Cancelar</button>\r\n      </div>\r\n    </form>\r\n    <div>\r\n      <button type=\"button\" @click=\"togglePasswordForm\">Cambiar contraseña</button>\r\n    </div>\r\n    <change-password-form v-if=\"showPasswordForm\" @change-password=\"changePassword\" @cancel-password-change=\"cancelPasswordChange\" />\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport BarriosService from \"@/services/BarriosService\";\r\nimport store from \"@/store\";\r\nimport ChangePasswordForm from \"@/components/ChangePasswordForm.vue\";\r\nimport NavPaseador from \"@/components/Nav/NavPaseador.vue\";\r\n\r\nexport default {\r\n  name: \"PerfilPaseador\",\r\n  components: {\r\n    ChangePasswordForm,\r\n    NavPaseador,\r\n  },\r\n  data() {\r\n    return {\r\n      paseador: {},\r\n      showForm: false,\r\n      updatedPaseadorData: {},\r\n      barrios: [],\r\n      showPasswordForm: false,\r\n      changePassword: false,\r\n      cancelPasswordChange: false,\r\n      \r\n    };\r\n  },\r\n  mounted() {\r\n    const paseadorId = store.getters.getClientId;\r\n    this.fetchPaseador(paseadorId);\r\n    this.fetchBarrios();\r\n  },\r\n  methods: {\r\n    fetchPaseador(paseadorId) {\r\n      PaseadoresService.get(paseadorId)\r\n        .then((response) => {\r\n          this.paseador = response.data;\r\n          this.updatedPaseadorData = { ...this.paseador };\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al obtener el perfil del paseador: \", error);\r\n        });\r\n    },\r\n    fetchBarrios() {\r\n      BarriosService.getAll()\r\n        .then((response) => {\r\n          this.barrios = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al obtener la lista de barrios: \", error);\r\n        });\r\n    },\r\n    showEditForm() {\r\n      this.showForm = true;\r\n    },\r\n    updateProfile() {\r\n      const paseadorId = store.getters.getClientId;\r\n      PaseadoresService.update(paseadorId, this.updatedPaseadorData)\r\n        .then(() => {\r\n          const barrioId = this.updatedPaseadorData.id_barrio;\r\n          const barrio = this.barrios.find((b) => b.id === barrioId);\r\n          this.paseador.nombre_barrio = barrio ? barrio.nombre : '';\r\n\r\n          this.showForm = false;\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al actualizar el perfil del paseador: \", error);\r\n        });\r\n    },\r\n    cancelEdit() {\r\n      this.showForm = false;\r\n    },\r\n    togglePasswordForm() {\r\n      this.showPasswordForm = !this.showPasswordForm;\r\n    },\r\n    cancelPasswordForm() {\r\n      this.showPasswordForm = false;\r\n    },\r\n    getImageUrl(filename) {\r\n  if (filename) {\r\n    //const imagePath = filename.replace(/\\\\/g, '/').replace('/vue', '');\r\n    const imagePath = filename.replace('vue', '').replace('public', '');\r\n    const baseUrl = window.location.origin;\r\n    //const imageUrl = imagePath.startsWith('/vue') ? imagePath.substr(1) : imagePath;\r\n    return `${baseUrl}/${imagePath}`;\r\n  }\r\n}\r\n\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.perfil-paseador {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput,\r\ntextarea,\r\nselect {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}