;MAAW;AAAT,IAAA,QAAQ,EAAR;AAAS,GAAvD,E,IAAA,E;;AAAA,G,iBAAuC,KAAA,CAAA,K,GACnC,CAHN,C,EAIA,YAAA,CAMM,KANN,EAAA,WAAA,EAMM,CALJ,WAKI,E,gBAJJ,YAAA,CAGS,QAHT,EAGS;AAHD,IAAA,EAAE,EAAC,MAGF;;aAHkB,KAAA,CAAA,IAAA,GAAI,M;MAGtB;AAHwB,IAAA,QAAQ,EAAR;AAGxB,GAHT,E,CACE,W,qBACA,YAAA,CAAoF,SAApF,EAAoF,IAApF,EAAoF,WAAA,CAA7D,QAAA,CAAA,cAA6D,EAA/C,UAAtB,IAAsB,EAAlB;yBAAnB,YAAA,CAAoF,QAApF,EAAoF;AAA5C,MAAA,GAAG,EAAE,IAAuC;AAAhC,MAAA,KAAK,EAAE;AAAyB,KAApF,E,iBAAoE,I,CAApE,EAAwE;AAAA;AAAxE,MAAwE,CAAA,OAAA,CAAxE,C;GAAoF,CAApF,E;;AAAA,G,EAFF,E;;AAAA,G,mBAA2B,KAAA,CAAA,I,GAIvB,CANN,C,EAOA,W,EACS,KAAA,CAAA,Y,kBAAT,YAAA,CAAmE,GAAnE,EAAA,WAAA,EAAmE,gBAAA,CAAnB,KAAA,CAAA,YAAmB,CAAnE,EAA4D;AAAA;AAA5D,G,sCA3BF,E;;AAAA,GA6BI,CA/BN,C","sourcesContent":["<template>\r\n  <div class=\"add-reserva\" style=\"justify-content: center;\">\r\n    <h1>Crear Reserva</h1>\r\n    <form @submit.prevent=\"crearReserva\">\r\n      <div class=\"form-group\">\r\n        <label for=\"paseador\">Paseador:</label>\r\n        <select id=\"paseador\" v-model=\"idPaseador\" required>\r\n          <option value=\"\">Seleccione un paseador</option>\r\n          <option v-for=\"paseador in paseadores\" :key=\"paseador.id\" :value=\"paseador.id\">{{ paseador.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"cliente\">Cliente:</label>\r\n        <select id=\"cliente\" v-model=\"idCliente\" required>\r\n          <option value=\"\">Seleccione un cliente</option>\r\n          <option v-for=\"cliente in clientes\" :key=\"cliente.id\" :value=\"cliente.id\">{{ cliente.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"fecha\">Fecha:</label>\r\n        <input type=\"text\" id=\"fecha\" v-model=\"fecha\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"hora\">Hora:</label>\r\n        <select id=\"hora\" v-model=\"hora\" required>\r\n          <option value=\"\">Seleccione una hora</option>\r\n          <option v-for=\"hour in availableHours\" :key=\"hour\" :value=\"hour\">{{ hour }}</option>\r\n        </select>\r\n      </div>\r\n      <button style=\"background: black !important;\" type=\"submit\">Crear Reserva</button>\r\n      <p v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport flatpickr from 'flatpickr';\r\nimport 'flatpickr/dist/flatpickr.css';\r\n\r\nimport ReservasService from \"@/services/ReservasService\";\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport ClientesService from \"@/services/ClientesService\";\r\n\r\nexport default {\r\n  name: \"AddReserva\",\r\n  data() {\r\n    return {\r\n      idPaseador: \"\",\r\n      idCliente: \"\",\r\n      fecha: \"\",\r\n      hora: \"\",\r\n      paseadores: [],\r\n      clientes: [],\r\n      errorMessage: \"\", // Added error message state\r\n    };\r\n  },\r\n  mounted() {\r\n    flatpickr('#fecha', {\r\n      minDate: 'today',\r\n      maxDate: new Date().fp_incr(30), // Restrict to a maximum of 30 days from today\r\n    });\r\n\r\n    this.fetchPaseadores();\r\n    this.fetchClientes();\r\n  },\r\n  computed: {\r\n    availableHours() {\r\n      // Generate an array of available hours from 7 to 20\r\n      const hours = [];\r\n      for (let i = 7; i <= 20; i++) {\r\n        hours.push(`${i}:00`);\r\n      }\r\n      return hours;\r\n    },\r\n  },\r\n  methods: {\r\n    fetchPaseadores() {\r\n      PaseadoresService.getAll()\r\n        .then((response) => {\r\n          this.paseadores = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    fetchClientes() {\r\n      ClientesService.getAll()\r\n        .then((response) => {\r\n          this.clientes = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    crearReserva() {\r\n      if (!this.validateReservation()) {\r\n        return;\r\n      }\r\n\r\n      const nuevaReserva = {\r\n        id_paseador: this.idPaseador,\r\n        id_cliente: this.idCliente,\r\n        fecha: this.fecha,\r\n        hora: this.hora,\r\n      };\r\n\r\n      ReservasService.create(nuevaReserva)\r\n        .then((response) => {\r\n          console.log(\"Reserva creada exitosamente: \", response.data);\r\n          // Realizar acciones adicionales después de crear una reserva (redireccionar, mostrar mensaje, etc.)\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al crear la reserva: \", error);\r\n          // Manejar el error y mostrar un mensaje de error al usuario si es necesario\r\n        });\r\n    },\r\n    validateReservation() {\r\n      // Check if hora is selected\r\n      if (!this.hora) {\r\n        this.errorMessage = \"Por favor, seleccione una hora.\";\r\n        return false;\r\n      }\r\n\r\n      // Check if paseador has reached the limit of 6 reservations for the same fecha and hora\r\n      const reservationsCount = this.paseadores.filter((paseador) => {\r\n        return paseador.id === this.idPaseador && paseador.fecha === this.fecha && paseador.hora === this.hora;\r\n      }).length;\r\n\r\n      if (reservationsCount >= 6) {\r\n        this.errorMessage = \"El paseador seleccionado ha alcanzado el límite de 6 reservas para la misma fecha y hora.\";\r\n        return false;\r\n      }\r\n\r\n      // Clear error message if validation passes\r\n      this.errorMessage = \"\";\r\n      return true;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.add-reserva {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\nselect,\r\ninput[type=\"text\"] {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  margin-top: 5px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}