AA,G,EAFF,E;;AAAA,G,mBAA8B,KAAA,CAAA,S,GAI1B,CANN,C,EAOA,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,WAEI,E,gBADJ,YAAA,CAA6E,OAA7E,EAA6E;AAAtE,IAAA,IAAI,EAAC,MAAiE;AAA1D,IAAA,EAAE,EAAC,OAAuD;;aAAtC,KAAA,CAAA,KAAA,GAAK,M;MAAiC;AAA/B,IAAA,QAAQ,EAAR,EAA+B;AAArB,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAgB,GAA7E,E,IAAA,E;;AAAA,G,iBAAuC,KAAA,CAAA,K,GACnC,CAHN,C,EAIA,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,WAEI,E,gBADJ,YAAA,CAAqD,OAArD,EAAqD;AAA9C,IAAA,IAAI,EAAC,MAAyC;AAAlC,IAAA,EAAE,EAAC,MAA+B;;aAAf,KAAA,CAAA,IAAA,GAAI,M;MAAW;AAAT,IAAA,QAAQ,EAAR;AAAS,GAArD,E,IAAA,E;;AAAA,G,iBAAsC,KAAA,CAAA,I,GAClC,CAHN,C,EAIA,W,CAvBF,E;;AAAA,GAyBI,CA3BN,C","sourcesContent":["<template>\r\n  <div class=\"add-reserva\" style=\"justify-content: center;\">\r\n    <h1>Crear Reserva</h1>\r\n    <form @submit.prevent=\"crearReserva\">\r\n      <div class=\"form-group\">\r\n        <label for=\"paseador\">Paseador:</label>\r\n        <select id=\"paseador\" v-model=\"idPaseador\" required>\r\n          <option value=\"\">Seleccione un paseador</option>\r\n          <option v-for=\"paseador in paseadores\" :key=\"paseador.id\" :value=\"paseador.id\">{{ paseador.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"cliente\">Cliente:</label>\r\n        <select id=\"cliente\" v-model=\"idCliente\" required>\r\n          <option value=\"\">Seleccione un cliente</option>\r\n          <option v-for=\"cliente in clientes\" :key=\"cliente.id\" :value=\"cliente.id\">{{ cliente.nombre }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"fecha\">Fecha:</label>\r\n        <input type=\"date\" id=\"fecha\" v-model=\"fecha\" required @input=\"validateDate\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"hora\">Hora:</label>\r\n        <input type=\"time\" id=\"hora\" v-model=\"hora\" required>\r\n      </div>\r\n      <button style=\"background: black !important;\" type=\"submit\">Crear Reserva</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// eslint-disable\r\nimport ReservasService from \"@/services/ReservasService\";\r\nimport PaseadoresService from \"@/services/PaseadoresService\";\r\nimport ClientesService from \"@/services/ClientesService\";\r\n\r\nexport default {\r\n  name: \"AddReserva\",\r\n  data() {\r\n    return {\r\n      idPaseador: \"\",\r\n      idCliente: \"\",\r\n      fecha: \"\",\r\n      hora: \"\",\r\n      paseadores: [],\r\n      clientes: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchPaseadores();\r\n    this.fetchClientes();\r\n  },\r\n  methods: {\r\n    fetchPaseadores() {\r\n      PaseadoresService.getAll()\r\n        .then((response) => {\r\n          this.paseadores = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    fetchClientes() {\r\n      ClientesService.getAll()\r\n        .then((response) => {\r\n          this.clientes = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    crearReserva() {\r\n      const nuevaReserva = {\r\n        id_paseador: this.idPaseador,\r\n        id_cliente: this.idCliente,\r\n        fecha: this.fecha,\r\n        hora: this.hora,\r\n      };\r\n\r\n      ReservasService.create(nuevaReserva)\r\n        .then((response) => {\r\n          console.log(\"Reserva creada exitosamente: \", response.data);\r\n          // Realizar acciones adicionales después de crear una reserva (redireccionar, mostrar mensaje, etc.)\r\n        })\r\n        .catch((error) => {\r\n          console.log(\"Error al crear la reserva: \", error);\r\n          // Manejar el error y mostrar un mensaje de error al usuario si es necesario\r\n        });\r\n    },\r\n    validateDate() {\r\n      const selectedDate = new Date(this.fecha);\r\n      const currentDate = new Date();\r\n\r\n      if (selectedDate.getDay() === 0) { // Check if Sunday is selected\r\n        alert(\"Los domingos no están permitidos. Por favor, elija otra fecha.\");\r\n        this.fecha = \"\"; // Clear the selected date\r\n      } else if (selectedDate < currentDate) { // Check if the selected date is before the current date\r\n        alert(\"No se permiten fechas anteriores a la fecha actual. Por favor, elija otra fecha.\");\r\n        this.fecha = \"\"; // Clear the selected date\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.add-reserva {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\nselect,\r\ninput[type=\"date\"],\r\ninput[type=\"time\"] {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}