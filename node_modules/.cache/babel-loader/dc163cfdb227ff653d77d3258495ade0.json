MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,KAAA,IAAA,QAAA,CAAA,KAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAe,CAAA,SAAA,CAAf,CAAA;AAUN,GAVP,E,CACE,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,mBAAA,CAAwD,OAAxD,EAAwD;AAAjD,IAAA,IAAI,EAAC,OAA4C;AAApC,IAAA,EAAE,EAAC,OAAiC;;aAAhB,KAAA,CAAA,K,GAAK,M;MAAW;AAAT,IAAA,QAAQ,EAAR;AAAS,GAAxD,E,IAAA,E;;AAAA,G,iBAAwC,KAAA,CAAA,K,GACpC,CAHN,C,EAIA,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,UAEI,E,gBADJ,mBAAA,CAAiE,OAAjE,EAAiE;AAA1D,IAAA,IAAI,EAAC,UAAqD;AAA1C,IAAA,EAAE,EAAC,UAAuC;;aAAnB,KAAA,CAAA,Q,GAAQ,M;MAAW;AAAT,IAAA,QAAQ,EAAR;AAAS,GAAjE,E,IAAA,E;;AAAA,G,iBAA8C,KAAA,CAAA,Q,GAC1C,CAHN,C,EAIA,U,CATF,E;;AAAA,GAWI,CAbN,C","sourcesContent":["<template>\r\n  <div class=\"login-cliente\">\r\n    <h1>Iniciar sesión como Cliente</h1>\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input type=\"email\" id=\"email\" v-model=\"email\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">Contraseña:</label>\r\n        <input type=\"password\" id=\"password\" v-model=\"password\" required>\r\n      </div>\r\n      <button type=\"submit\">Iniciar sesión</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ClientesService from \"@/services/ClientesService\";\r\nimport store from \"@/store\";\r\n\r\n\r\nexport default {\r\n  name: \"LoginCliente\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      const credentials = {\r\n        email: this.email,\r\n        password: this.password\r\n      };\r\n\r\n      ClientesService.login(credentials)\r\n        .then(response => {\r\n          const clientId = response.data.clientId;\r\n          store.commit('setClientId', clientId);\r\n          localStorage.setItem(\"Authorization\", 'Bearer ' + response.data.token);\r\n          // Redireccionar a la página de cliente autenticado\r\n          this.$router.push(\"/lista-paseadores\");\r\n\r\n          // Establecer notificación y mensaje en el store\r\n          store.commit('setNotification', 'success');\r\n          store.commit('setMessage', 'Inicio de sesión exitoso');\r\n        })\r\n        .catch(error => {\r\n          console.log(\"Error al iniciar sesión como cliente: \", error);\r\n          // Establecer notificación y mensaje de error en el store\r\n          store.commit('setNotification', 'error');\r\n          store.commit('setMessage', 'Error al iniciar sesión');\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-cliente {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.password-input {\r\n  position: relative;\r\n}\r\n\r\n.password-input button {\r\n  position: absolute;\r\n  top: 50%;\r\n  right: 5px;\r\n  transform: translateY(-50%);\r\n  background-color: transparent;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.password-input button:focus {\r\n  outline: none;\r\n}\r\n\r\n.password-input img {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}