seadorName(paseadorId) {\r\n      const paseador = this.paseadores.find(p => p.id === paseadorId);\r\n      return paseador ? paseador.name : '';\r\n    },*/\r\n    formatDate(date) {\r\n      return moment(date).format(\"DD/MM/YYYY\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.reservas-cliente {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.reserva-item {\r\n  margin-bottom: 10px;\r\n  background-color: #f0f0f0;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"]}]}