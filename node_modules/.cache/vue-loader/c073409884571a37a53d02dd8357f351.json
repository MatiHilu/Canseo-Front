\n          <input type=\"text\" id=\"telefono\" v-model=\"telefono\" required>\r\n        </div>\r\n        <button style=\"background: black !important;\" type=\"submit\">Agregar</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  // eslint-disable\r\n  import PaseadoresService from \"@/services/PaseadoresService\";\r\n\r\n  export default {\r\n    name: \"AddPaseador\",\r\n    data() {\r\n      return {\r\n        nombre: \"\",\r\n        email: \"\",\r\n        telefono: \"\"\r\n      };\r\n    },\r\n    methods: {\r\n      agregarPaseador() {\r\n        const nuevoPaseador = {\r\n          nombre: this.nombre,\r\n          email: this.email,\r\n          telefono: this.telefono\r\n        };\r\n  \r\n        PaseadoresService.create(nuevoPaseador)\r\n          .then(response => {\r\n            console.log(\"Paseador agregado exitosamente: \", response.data);\r\n            // Realizar acciones adicionales después de agregar un paseador (redireccionar, mostrar mensaje, etc.)\r\n          })\r\n          .catch(error => {\r\n            console.log(\"Error al agregar el paseador: \", error);\r\n            // Manejar el error y mostrar un mensaje de error al usuario si es necesario\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .add-paseador {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  h1 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input {\r\n    width: 100%;\r\n    padding: 5px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  button {\r\n    padding: 8px 12px;\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  </style>\r\n  "]}]