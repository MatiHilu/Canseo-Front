h/Desktop/CANSEO V3/vue/src/components/PerfilCliente.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div class=\"perfil-cliente\">\r\n      <h1>Perfil del Cliente</h1>\r\n      <div>\r\n        <p>Nombre: {{ cliente.nombre }}</p>\r\n        <p>Apellido: {{ cliente.apellido }}</p>\r\n        <p>Email: {{ cliente.email }}</p>\r\n        <p>Teléfono: {{ cliente.telefono }}</p>\r\n        <p>Dirección: {{ cliente.direccion }}</p>\r\n        <p>Descripción: {{ cliente.descripcion }}</p>\r\n        <p>Mi Barrio: {{ cliente.nombre_barrio }}</p>\r\n        <p>Foto: {{ cliente.foto_perfil }}</p>\r\n        <img width=\"150\" :src=\"getImageUrl(cliente.foto_perfil)\" alt=\"Foto de perfil\">\r\n      \r\n      </div>\r\n      <button @click=\"showEditForm\">Editar perfil</button>\r\n      <form v-if=\"showForm\" @submit.prevent=\"updateProfile\">\r\n        <div class=\"form-group\">\r\n          <label for=\"nombre\">Nombre:</label>\r\n          <input type=\"text\" id=\"nombre\" v-model=\"updatedClienteData.nombre\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"apellido\">Apellido:</label>\r\n          <input type=\"text\" id=\"apellido\" v-model=\"updatedClienteData.apellido\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">Email:</label>\r\n          <input type=\"email\" id=\"email\" v-model=\"updatedClienteData.email\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"telefono\">Teléfono:</label>\r\n          <input type=\"text\" id=\"telefono\" v-model=\"updatedClienteData.telefono\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"direccion\">Dirección:</label>\r\n          <input type=\"text\" id=\"direccion\" v-model=\"updatedClienteData.direccion\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"descripcion\">Descripción:</label>\r\n          <textarea id=\"descripcion\" v-model=\"updatedClienteData.descripcion\"></textarea>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"barrio\">Barrio:</label>\r\n          <select id=\"barrio\" v-model=\"updatedClienteData.id_barrio\" required>\r\n            <option v-for=\"barrio in barrios\" :value=\"barrio.id\" :key=\"barrio.id\">{{ barrio.nombre }}</option>\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <button type=\"submit\">Guardar cambios</button>\r\n          <button type=\"button\" @click=\"cancelEdit\">Cancelar</button>\r\n        </div>\r\n      </form>\r\n      <div>\r\n        <button type=\"button\" @click=\"togglePasswordForm\">Cambiar contraseña</button>\r\n      </div>\r\n      <change-password-form v-if=\"showPasswordForm\" @change-password=\"changePassword\" @cancel-password-change=\"cancelPasswordChange\" />\r\n    </div>\r\n  </template>\r\n  \r\n  \r\n  <script>\r\n  import ClienteService from \"@/services/ClientesService\";\r\n  import BarriosService from \"@/services/BarriosService\";\r\n  import store from \"@/store\";\r\n  import ChangePasswordForm from \"@/components/ChangePasswordForm.vue\";\r\n  \r\n  export default {\r\n    name: \"PerfilCliente\",\r\n    components: {\r\n      ChangePasswordForm\r\n    },\r\n    data() {\r\n      return {\r\n        cliente: {},\r\n        showForm: false,\r\n        updatedClienteData: {},\r\n        barrios: [],\r\n        showPasswordForm: false,\r\n        changePassword: false,\r\n        cancelPasswordChange: false,\r\n        \r\n      };\r\n    },\r\n    mounted() {\r\n      const clienteId = store.getters.getClientId;\r\n      this.fetchCliente(clienteId);\r\n      this.fetchBarrios();\r\n    },\r\n    methods: {\r\n      fetchCliente(clienteId) {\r\n        ClienteService.get(clienteId)\r\n          .then((response) => {\r\n            this.cliente = response.data;\r\n            this.updatedClienteData = { ...this.cliente };\r\n          })\r\n          .catch((error) => {\r\n            console.log(\"Error al obtener el perfil del cliente: \", error);\r\n          });\r\n      },\r\n      fetchBarrios() {\r\n        BarriosService.getAll()\r\n          .then((response) => {\r\n            this.barrios = response.data;\r\n          })\r\n          .catch((error) => {\r\n            console.log(\"Error al obtener la lista de barrios: \", error);\r\n          });\r\n      },\r\n      showEditForm() {\r\n        this.showForm = true;\r\n      },\r\n      updateProfile() {\r\n        const clienteId = store.getters.getClientId;\r\n        ClienteService.update(clienteId, this.updatedClienteData)\r\n          .then(() => {\r\n            const barrioId = this.updatedClienteData.id_barrio;\r\n            const barrio = this.barrios.find((b) => b.id === barrioId);\r\n            this.cliente.nombre_barrio = barrio ? barrio.nombre : '';\r\n  \r\n            this.showForm = false;\r\n          })\r\n          .catch((error) => {\r\n            console.log(\"Error al actualizar el perfil del cliente: \", error);\r\n          });\r\n      },\r\n      cancelEdit() {\r\n        this.showForm = false;\r\n      },\r\n      togglePasswordForm() {\r\n        this.showPasswordForm = !this.showPasswordForm;\r\n      },\r\n      cancelPasswordForm() {\r\n        this.showPasswordForm = false;\r\n      },\r\n      getImageUrl(filename) {\r\n    if (filename) {\r\n      //const imagePath = filename.replace(/\\\\/g, '/').replace('/vue', '');\r\n      const imagePath = filename.replace('vue', '').replace('public', '');\r\n      const baseUrl = window.location.origin;\r\n      //const imageUrl = imagePath.startsWith('/vue') ? imagePath.substr(1) : imagePath;\r\n      return `${baseUrl}/${imagePath}`;\r\n    }\r\n  }\r\n  \r\n  \r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .perfil-cliente {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  h1 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input,\r\n  textarea,\r\n  select {\r\n    width: 100%;\r\n    padding: 5px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  button {\r\n    padding: 8px 12px;\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  </style>\r\n  "]}]}